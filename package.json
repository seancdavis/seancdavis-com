    "analytics": "^0.7.3",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "imgix-core-js": "^2.3.2",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-attrs": "^4.0.0",
    "netlify-plugin-a11y": "^0.0.12",
    "netlify-plugin-checklinks": "^4.1.1",
    "netlify-plugin-inline-critical-css": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "postcss-custom-properties": "^10.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "slugify": "^1.4.7",
    "tailwindcss": "^1.9.6",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "build": "npm-run-all clean build:css build:html build:js",
    "build:css": "postcss src/_includes/css/index.css -o src/css/styles.css",
    "build:html": "eleventy --quiet",
    "build:js": "webpack",
    "clean": "rm -rf dist",
    "dev": "npm-run-all --parallel develop:css develop:html develop:js",
    "develop": "dev",
    "develop:css": "postcss src/_includes/css/index.css -o src/css/styles.css --watch",
    "develop:html": "eleventy --serve --port 8000",
    "develop:js": "webpack",
    "start": "develop",
    "test": "ELEVENTY_ENV=test jest"
  },
  "devDependencies": {
    "pnpm": "^6.0.1"
  }
}
