  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@analytics/google-analytics": "^0.4.1",
    "analytics": "^0.5.5",
    "autoprefixer": "^9.7.6",
    "cssnano": "^4.1.10",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "imgix-core-js": "^2.3.1",
    "jest": "^26.2.2",
    "lodash": "^4.17.19",
    "markdown-it": "^12.0.2",
    "markdown-it-attrs": "^3.0.3",
    "netlify-plugin-a11y": "^0.0.12",
    "netlify-plugin-checklinks": "^4.1.1",
    "netlify-plugin-inline-critical-css": "^1.1.3",
    "netlify-plugin-subfont": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-custom-properties": "^10.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "slugify": "^1.4.6",
    "tailwindcss": "^1.4.4",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "npm-run-all clean build:css build:html build:js",
    "build:css": "postcss src/_includes/css/index.css -o src/css/styles.css",
    "build:html": "eleventy --quiet",
    "build:js": "webpack",
    "clean": "rm -rf dist",
    "develop": "npm-run-all --parallel develop:css develop:html develop:js",
    "develop:css": "postcss src/_includes/css/index.css -o src/css/styles.css --watch",
    "develop:html": "eleventy --serve --port 8000",
    "develop:js": "webpack",
    "start": "develop",
    "test": "ELEVENTY_ENV=test jest"
  }
}
