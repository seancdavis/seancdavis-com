    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "@analytics/google-analytics": "^0.5.3",
    "@hirusi/eleventy-plugin-safe-external-links": "^0.14.3",
    "@imgix/js-core": "^3.1.3",
    "@lhci/cli": "^0.8.0",
    "@netlify/plugin-sitemap": "^0.8.1",
    "@seancdavis/netlify-plugin-jest": "^0.1.0",
    "analytics": "^0.7.15",
    "autoprefixer": "^10.3.7",
    "aws-sdk": "^2.1010.0",
    "chalk": "^4.1.1",
    "cssnano": "^5.0.6",
    "glob": "^7.1.7",
    "html-minifier": "^4.0.0",
    "http-server": "^13.0.0",
    "jest": "^27.0.4",
    "jest-image-snapshot": "^4.5.0",
    "jest-puppeteer": "^6.0.0",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^8.0.3",
    "markdown-it-attrs": "^4.1.0",
    "netlify-plugin-a11y": "^0.0.12",
    "netlify-plugin-checklinks": "^4.1.1",
    "netlify-plugin-inline-critical-css": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.2",
    "postcss-cli": "^9.0.1",
    "postcss-custom-properties": "^12.0.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "slugify": "^1.6.1",
    "tailwindcss": "^2.1.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "scripts": {
    "build": "npm-run-all clean build:css build:html build:js",
    "build:css": "postcss src/_includes/css/index.css -o src/css/styles.css",
    "build:html": "eleventy --quiet",
    "build:js": "webpack",
    "clean": "rm -rf dist",
    "dev": "npm-run-all --parallel develop:css develop:html develop:js",
    "develop": "dev",
    "develop:css": "postcss src/_includes/css/index.css -o src/css/styles.css --watch",
    "develop:html": "eleventy --serve --port 8000",
    "develop:js": "webpack",
    "pretest:visual": "DISABLE_PARTICLES=true DISABLE_TYPEWRITER=true npm run build",
    "start": "develop",
    "test": "ELEVENTY_ENV=test jest",
    "test:visual": "JEST_CONFIG=visual npm run test"
  }
}
